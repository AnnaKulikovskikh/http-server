{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-restricted-syntax": ["off", "ForOfStatement"],
        "class-methods-use-this": [0, { "exceptMethods": ["addDescription", "getTime"] }],
        "prefer-destructuring": ["error", {"object": true, "array": false}],
        "no-unused-vars": ["off", { "vars": "all"}]
    }
}
